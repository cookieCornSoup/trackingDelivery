<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trackingdelivery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".View.OtherAti.SplashActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".View.Delivery.DeliveryDetail"></activity>
        <activity android:name=".View.Delivery.DeliverySearch"></activity>
        <activity android:name=".View.OtherAti.MainActivity"/>
        <activity android:name=".View.Login.MainLoginActivity"/>
        <activity android:name=".View.Login.ChangePasswordActivity"/>
        <activity android:name=".View.Login.EmailLoginActivity"/>
    </application>

</manifest>